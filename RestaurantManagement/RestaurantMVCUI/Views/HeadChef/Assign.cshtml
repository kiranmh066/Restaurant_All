@model IEnumerable<RestaurantEntity.Employee>
@{
    Layout = "~/Views/Shared/_LayoutHeadChef.cshtml";
}
<body style="background-color:#5A3B5D">
    <div class="container">
<div>
    <a asp-controller="HeadChef" asp-action="Index">Home</a>
    <hr />
</div>
<h2 style="color:white">Index of HeadChef</h2>
@foreach(var item in Model)
{   
    string a =Convert.ToString(TempData["speciality"]);
    TempData.Keep();
    if (item.EmpSpeciality == a)
    {
        <table>
            <thead>
                <tr class="text-white">
                <th>Employee Id</th>
                <th>Employee Name</th>
                <th>Employee Speciality</th>
                <th>Assign</th>
        </tr>
         
        </thead>
        <tbody>
                <tr class ="text-white">
                <td>@item.EmpId</td>
                <td>@item.EmpName</td>
                <td>@item.EmpSpeciality</td>

                    <td>
                        <a asp-controller="Chef" asp-action="Index"
                           asp-route-EmpId="@item.EmpId"><img src="https://www.svgrepo.com/show/189280/tasks-tick.svg" style="width:50px;height:50px"><span>&nbsp;</span>Assign to Chef</a>
                    </td>

            </tr>
          
        
           
        </tbody>

    </table>}
}
</div>
</body>